@model List<Shipping>

    <div class="container">
        <div class="row">
            <a class="btn btn-succes" asp-action="Create">Create</a>
        <table class="table table-striped table-dark">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Title</th>
                    <th scope="col">Description</th>
                    <th scope="col">Icon</th>
                    <th scope="col">Actions</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Shipping shipping in Model)
                {

                    <tr>
                        <th scope="row">@shipping.Id</th>
                        <td>@shipping.Title</td>
                        <td>@shipping.Description</td>
                        <td> <img src="~/assets/images/@shipping.Icon" width="30" height="30" alt="Alternate Text" /></td>
                 <td>
                     <a class="btn btn-warning">Update</a>
                     <a asp-action="Delete" asp-route-id="@shipping.Id" class="btn btn-danger">Delete</a>
                 </td>
                    </tr>
                }
            </tbody>
        </table>
        </div>
    </div>

